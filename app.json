{
  "expo": {
    "name": "iSeaTree",
    "slug": "iSeaTree",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "CAMERA"],
      "package": "com.treemama.iseatree",
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      }
    },
    "ios": {
	"bundleIdentifier": "com.treemama.iseatree",
	"buildNumber": "1",
	"infoPlist": {
	    "NSCameraUsageDescription": "Take a photo and submit it with your iSeaTree reports.",
	    "NSLocationWhenInUseUsageDescription": "Record the location of your iSeaTree reports, and show your location on a map",
	    "NSPhotoLibraryUsageDescription": "Submit an existing photo with your iSeaTree report.",
	},
    },
    "entryPoint": "./src/App.tsx",
    "description": ""
  }
}
