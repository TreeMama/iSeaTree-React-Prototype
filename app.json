{
	"expo": {
		"name": "iSeaTree",
		"slug": "iSeaTree",
		"privacy": "public",
		"platforms": [ "ios", "android" ],
		"version": "2.1",
		"orientation": "default",
		"icon": "./assets/icon.png",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [ "**/*" ],
		"android": {
			"permissions": [ "ACCESS_COARSE_LOCATION", "ACCESS_FINE_LOCATION", "CAMERA" ],
			"package": "com.treemama.iseatree",
			"versionCode": 48,
			"config": {
				"googleMaps": {
					"apiKey": "---------------------------"
				}
			},
			"icon": "./assets/icon.png",
			"splash": {
				"mdpi": "./assets/android-splash/splash.png",
				"hdpi": "./assets/android-splash/splash_hdpi.png",
				"xhdpi": "./assets/android-splash/splash_xhdpi.png",
				"xxhdpi": "./assets/android-splash/splash_xxhdpi.png",
				"resizeMode": "contain",
				"backgroundColor": "#ffffff"
			},
			"adaptiveIcon": {
				"foregroundImage": "./assets/android_icon_forground.png",
				"backgroundColor": "#FFFFF",
				"backgroundImage": "./assets/android_icon_background.png"
			}
		},
		"ios": {
			"icon": "./assets/icon.png",
			"bundleIdentifier": "com.treemama.iseatree",
			"buildNumber": "48",
			"supportsTablet": true,
			"infoPlist": {
				"NSCameraUsageDescription": "We use the camera to allow you to take a photo and submit it with your iSeaTree reports.",
				"NSLocationWhenInUseUsageDescription": "We attach your location to your iSeaTree reports, and show your location on a map",
				"NSPhotoLibraryUsageDescription": "You have asked to upload a tree photo from your photo library."
			}
		},
		"entryPoint": "./src/App.tsx",
		"description": ""
	}
}
